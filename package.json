{
  "name": "taco",
  "description": "Comparative visualization of large tabular data using Caleydo Web",
  "version": "2.0.0-alpha.0",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/taco",
  "homepage": "http://caleydo.org",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_core": "*",
    "caleydo_i18n": "*",
    "caleydo_vis": "*",
    "caleydo_d3": "*",
    "caleydo_bootstrap_fontawesome": "*"
  },
  "caleydo": {
    "type": "app",
    "plugins": {
      "web": [
        {
          "type": "app",
          "id": "taco",
          "name": "TaCo"
        },
        {
          "type": "tacoView",
          "id": "DatasetSelector",
          "name": "Dataset Selector",
          "file": "DatasetSelector",
          "factory": "create"
        },
        {
          "type": "tacoView",
          "id": "Timeline",
          "name": "Timeline",
          "file": "Timeline",
          "factory": "create"
        }
      ]
    },
    "dependencies": {
      "web": {
        "bootstrap": "~3.3.4"
      }
    },
    "requirejs-config": {
      "paths": {
        "bootstrap": "${baseUrl}/bootstrap/dist/js/bootstrap"
      },
      "shim": {
        "bootstrap": [
          "jquery",
          "css!${baseUrl}/bootstrap/dist/css/bootstrap"
        ]
      }
    }
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
