{
  "name": "taco",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Reemh/taco",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_core": "*",
    "caleydo_vis": "*",
    "caleydo_d3": "*"
  },
  "caleydo": {
    "plugins": {
      "web": [
        {
          "type": "app",
          "id": "taco",
          "name": "Taco"
        },
        {
          "type": "datatype",
          "id": "diffstructure",
          "name": "Diff Structure",
          "file": "diffmatrix"
        },
        {
          "type": "vis",
          "id": "diffmatrixvis",
          "name": "Diff Structure Vis",
          "filter": "diffstructure",
          "file": "diff_heatmap",
          "factory": "createDiff"
        },
        {
          "type": "vis",
          "id": "diffplotvis",
          "name": "Diff Plot Vis",
          "filter": "diffstructure",
          "file": "diff_barplot"
        },
        {
          "type": "vis",
          "id": "diffpercentagebarvis",
          "name": "Diff Perc Bar Vis",
          "filter": "diffstructure",
          "file": "diff_percentagebar"
        }
      ]
    },
    "dependencies": {
      "web": {
        "underscore": "~1.8",
        "bootstrap": "~3.3.4",
        "toastr" : ">=2.1.2",
        "bootstrap-slider" :"*"
      }
    },
    "requirejs-config": {
      "paths": {
        "bootstrap": "${baseUrl}/bootstrap/dist/js/bootstrap",
        "bootstrap-slider": "${baseUrl}/seiyria-bootstrap-slider/js/bootstrap-slider"
      },
      "shim": {
        "bootstrap": [
          "jquery",
          "css!${baseUrl}/bootstrap/dist/css/bootstrap"
        ],
        "toastr": [
          "css!${baseUrl}/toastr/toastr.css"
        ],
        "bootstrap-slider": [
          "css!${baseUrl}/seiyria-bootstrap-slider/dist/css/bootstrap-slider.css"
        ]
      }
    }
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
