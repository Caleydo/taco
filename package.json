{
  "name": "taco",
  "version": "0.0.1",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_core": "*"
  },
  "caleydo": {
    "plugins": {
      "web": [
        {
          "type": "vis",
          "id": "taco-vis",
          "name": "Taco",
          "filter": "idk"
        },
        {
          "type": "app",
          "id": "taco",
          "name": "Taco"
        }
      ]
    },
    "dependencies": {
      "web": {
        "underscore": ">=1.6",
        "bootstrap": "~3.3.4"
      }
    },
    "requirejs-config": {
      "paths": {
        "bootstrap": "${baseUrl}/bootstrap/dist/js/bootstrap"
      },
      "shim": {
        "bootstrap": [
          "css!${baseUrl}/bootstrap/dist/css/bootstrap"
        ]
      }
    }
  },
  "publishConfig": {
    "registry": "http://dev.caleydo.org/nexus/content/repositories/caleydo-web/"
  }
}
