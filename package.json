{
  "name": "taco",
  "description": "Comparative visualization of large tabular data using Caleydo Web",
  "version": "2.0.0-alpha.0",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/taco",
  "homepage": "http://caleydo.org",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_core": "*",
    "caleydo_vis": "*",
    "caleydo_d3": "*",
    "caleydo_bootstrap_fontawesome": "*",
    "taco_server": "*"
  },
  "caleydo": {
    "type": "app",
    "plugins": {
      "web": [
        {
          "type": "app",
          "id": "taco",
          "name": "TaCo"
        },
        {
          "type": "tacoView",
          "id": "DataSetSelector",
          "name": "Data Set Selector",
          "file": "data_set_selector",
          "factory": "create"
        },
        {
          "type": "tacoView",
          "id": "Timeline",
          "name": "Timeline",
          "file": "timeline",
          "factory": "create"
        },
        {
          "type": "tacoView",
          "id": "HeatMap",
          "name": "Heat Map",
          "file": "heat_map",
          "factory": "create"
        }
      ]
    },
    "dependencies": {
      "web": {
        "bootstrap": "~3.3.4",
        "moment": "~2.15.1"
      }
    },
    "requirejs-config": {
      "paths": {
        "bootstrap": "${baseUrl}/bootstrap/dist/js/bootstrap",
        "moment": "${baseUrl}/moment/min/moment.min"
      },
      "shim": {
        "bootstrap": [
          "jquery",
          "css!${baseUrl}/bootstrap/dist/css/bootstrap"
        ]
      }
    }
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
